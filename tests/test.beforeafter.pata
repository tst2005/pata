#! ./bin/pata.sh

Require ../alpha/beforeafter

${PATA_MOD_PREFIX}begin

if false; then

echo foo | after
before | before | after | after
before | hexdump -C

fi

Aliaser() {
	echo "${PATA_MOD_PREFIX}$1";
}
middle="$(Aliaser middle)"
commit="$(Aliaser commit)"

